<h1>Books</h1>


<p>Find me in app/views/book/index.html.erb</p>


<h2>New book</h2>

<%= form_with model: @book, url: '/:id', method: :post do |f| %>

    <h4>Title</h4>
    <%= f.text_field :title %>
    
    <h4>Body</h4>
    <%= f.text_area :body %>
    
    <%= f.submit 'Create book' %>

<% end%>